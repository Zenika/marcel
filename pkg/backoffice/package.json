{
  "name": "backoffice",
  "version": "0.1.0",
  "private": true,
  "license": "Apache-2.0",
  "homepage": ".",
  "proxy": "http://localhost:8090",
  "dependencies": {
    "classnames": "^2.2.6",
    "connected-react-router": "^6.5.2",
    "hoc-react-loader": "^6.2.0",
    "immutadot": "^1.0.0",
    "immutadot-lodash": "^1.0.0",
    "lodash": "^4.17.19",
    "prop-types": "^15.7.2",
    "react": "^16.9.0",
    "react-color": "^2.13.0",
    "react-dom": "^16.9.0",
    "react-grid-layout": "https://github.com/EmrysMyrddin/react-grid-layout.git#temp_with_build",
    "react-redux": "^7.1.1",
    "react-redux-toastr": "^7.5.1",
    "react-router-dom": "^5.0.1",
    "react-scripts": "3.4.4",
    "react-sizeme": "^2.3.2",
    "react-sortable-hoc": "^1.10.1",
    "react-toolbox": "^2.0.0-beta.8",
    "redux": "^4.0.4",
    "redux-actions": "^2.0.3",
    "redux-logger": "^3.0.1",
    "redux-thunk": "^2.3.0",
    "reselect": "^4.0.0",
    "semver": "^6.3.0",
    "uuid": "^3.3.3"
  },
  "devDependencies": {
    "react-toolbox-themr": "^1.0.2"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "toolbox": "react-toolbox-themr",
    "lint": "eslint src"
  },
  "eslintConfig": {
    "extends": "react-app",
    "rules": {
      "no-console": [
        "error",
        {
          "allow": [
            "error"
          ]
        }
      ]
    }
  },
  "reactToolbox": {
    "include": [
      "BUTTON",
      "DATE_PICKER",
      "APP_BAR",
      "INPUT",
      "LIST",
      "SWITCH",
      "TABS",
      "CARD",
      "DROPDOWN",
      "PROGRESS_BAR"
    ],
    "customProperties": {
      "animation-duration": "0.3s",
      "color-accent": "var(--palette-pink-a200)",
      "color-accent-dark": "var(--palette-pink-700)",
      "color-primary-contrast": "var(--color-dark-contrast)",
      "color-accent-contrast": "var(--color-dark-contrast)"
    },
    "output": "src/assets/react-toolbox"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
